{
    "rules": {
        "no-console": 0,
        "semi": [ 2, "always" ],
        "func-names": 0,
        "no-param-reassign": [ 2, { "props": false } ],
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "global-require": 0,
        "import/no-unresolved": [ 2, { "ignore": [ "atom"] } ],
        "indent": ["error", 4],
        "max-len": 0
    },
    "extends": "airbnb-base",
    "globals": {
        "atom": "true"
    },
    "env": {
        "node": true
    }
}
